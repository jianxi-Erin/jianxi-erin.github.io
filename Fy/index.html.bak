.sports{
	width:200px;
	height:200PX;
	/*background:#fff;*/
	margin:50px auto 0;
	margin-left:-50px;
	border-radius:100px 100px 100px 100px;
	animation: sport 1s infinite;
}
@keyframes sport{
	0%{box-shadow:400px 0px 20px -80px green,
			   600px 0px 20px -80px green,
			   800px 0px 20px -80px green,
			   1000px 0px 20px -80px green;
			 }
	100%{box-shadow:200px 0px 20px -80px green,
			   400px 0px 20px -80px green,
			   600px 0px 20px -80px green,
			   800px 0px 20px -80px green;
			 }
	}
	
.sports:before,.sports:after{
	display:block;
	content:"";
	width:200px;
	height:100px;
	background:yellow;
	margin-left:200px;
}
.sports:before{
	border-radius:100px 100px 0 0;
	transform:rotate(0deg);
	animation: move 1s infinite;
}
.sports:after{
	border-radius:0 0 100px 100px;
	transform:rotate(0deg);
	animation: run 1s infinite;
}
@keyframes run{
	0%{
	transform:rotate(0deg);
	}
	50%{
	transform:rotate(30deg);
	}
	100%{
	transform:rotate(0deg);
	}
}
@keyframes move{
	0%{
	transform:rotate(0deg);
	}
	50%{
	transform:rotate(-30deg);
	}
	100%{
	transform:rotate(0deg);
	}
}
